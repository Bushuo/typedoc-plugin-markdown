{
  "name": "typedoc-hugo-theme",
  "version": "0.3.0",
  "description": "An extension of the Markdown theme from typedoc-plugin-markdown that prepends Hugo Front Matter variables in YAML format to pages.",
  "main": "dist/theme.js",
  "files": [
    "dist/"
  ],
  "bugs": {
    "url": "https://github.com/tgreyuk/typedoc-plugin-markdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-markdown.git",
    "directory": "packages/typedoc-hugo-theme"
  },
  "homepage": "https://github.com/tgreyuk/typedoc-plugin-markdown/tree/master/packages/typedoc-hugo-theme",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "npm run lint && npm run build",
    "build": "rm -rf ./dist && tsc",
    "test": "jest --colors",
    "build-and-test": "npm run build && npm run test",
    "demo": "rm -rf ./demo && npm run build && typedoc ../typedoc-plugin-markdown/test/stubs/src/index.ts --tsconfig ../typedoc-plugin-markdown/test/stubs/tsconfig.json --theme ./dist --out ./demo"
  },
  "author": "Thomas Grey",
  "license": "MIT",
  "peerDependencies": {
    "typedoc": ">=0.22.0",
    "typedoc-plugin-markdown": ">=3.11.0"
  },
  "keywords": [
    "typedoc",
    "hugo"
  ]
}
